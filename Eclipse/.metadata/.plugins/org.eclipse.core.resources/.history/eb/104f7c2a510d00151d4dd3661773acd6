package com.amit.utils;

/**
 *
 * @author Amit
 */
public class BST {
	
	public BST()
	{
		data = 0xFFFF;
		left = null;
		right = null;
		nodeCount = 0;		
	}
	
	public void add(int i_node_data)
	{
		if(nodeCount ==0)
		{
			data = i_node_data;
		}
		else
		{
			bool inserted = false;
			BST traverseToNode;
			BST insertingNode = new BST();
			
			while(!inserted)
			{
				if(data.)
					traverseToNode = left;
			}
		}
	}
	
	
	
	private T data;
	private BST left;
	private BST right;
	private int nodeCount;
	
    
}
